{
  "name": "STM32Ethernet",
  "keywords": "Ethernet",
  "description": "Arduino library to support Ethernet for STM32 based board.",
  "repository": {
    "type": "git",
    "url": "https://github.com/stm32duino/STM32Ethernet"
  },
  "version": "1.0.5",
  "frameworks": "arduino",
  "platforms": "ststm32",
  "build": {
    "libArchive": false,
    "flags": [
      "-I src/utility",
      "-I \"$PROJECTLIBDEPS_DIR/$PIOENV/STM32duino FreeRTOS/portable/CMSIS_RTOS\"",
      "-I \"$PROJECTLIBDEPS_DIR/$PIOENV/STM32duino FreeRTOS/src\""
    ]
  },
  "dependencies": [{
    "name": "LwIP",
    "version": "https://gitee.com/kaikong/LwIP.git"
  }, {
    "name": "ArduinoHttpClient",
    "version": "https://gitee.com/kaikong/ArduinoHttpClient.git"
  }, {
    "version": "https://gitee.com/kaikong/STM32FreeRTOS.git",
    "name": "STM32FreeRTOS"
  }]
}